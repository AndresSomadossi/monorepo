{
	"id": 3588295,
	"name": "Main",
	"target": "branch",
	"source_type": "Repository",
	"source": "AndresSomadossi/monorepo",
	"enforcement": "active",
	"conditions": { "ref_name": { "exclude": [], "include": ["~DEFAULT_BRANCH"] } },
	"rules": [
		{ "type": "creation" },
		{ "type": "deletion" },
		{ "type": "non_fast_forward" },
		{ "type": "required_linear_history" },
		{
			"type": "pull_request",
			"parameters": {
				"required_approving_review_count": 0,
				"dismiss_stale_reviews_on_push": true,
				"require_code_owner_review": true,
				"require_last_push_approval": false,
				"required_review_thread_resolution": false,
				"automatic_copilot_code_review_enabled": false,
				"allowed_merge_methods": ["squash"]
			}
		},
		{
			"type": "required_status_checks",
			"parameters": {
				"strict_required_status_checks_policy": true,
				"do_not_enforce_on_create": false,
				"required_status_checks": [
					{ "context": "ci-checks", "integration_id": 15368 },
					{ "context": "validate-issue", "integration_id": 15368 },
					{ "context": "staging-deploy", "integration_id": 15368 }
				]
			}
		},
		{
			"type": "required_deployments",
			"parameters": { "required_deployment_environments": ["staging"] }
		}
	],
	"bypass_actors": []
}
